#include <stdio.h>
void calcularTabuado(int n)
{
    
    int resultado;
    int i=0;
    for (int i = 1; i <= 9; i++)
    {
     
        resultado = i * n;
        printf("%i x %i = %i\n", n, i, resultado);
    }
    
}



int main()
{
    int n;
    printf("Digite um nÃºmero para receber sua tabuada: ");
    scanf("%i", &n);
    calcularTabuado (n);
}
